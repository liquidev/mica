# Tests operations and methods defined on the Dict type.

assert(["a": "b"] == ["a": "b"])
assert(["b": "b"] != ["a": "b", "x": "y"])

d = ["x": 1, "y": 2]

assert(d.clone == d)

assert(d.len == 2)
assert(!d.is_empty)
assert([:].is_empty)

assert(d.get("x") == 1)
assert(d.get("y") == 2)
assert(d.get("z") == nil)
assert(d.contains_key("x") and d.contains_key("y"))
assert(!d.contains_key("z"))

do
   di = d.clone
   assert(di.insert("x", 2) == 1)
   assert(di == ["x": 2, "y": 2])
end

do
   di = d.clone
   assert(di.remove("x") == 1)
   assert(di == ["y": 2])
end

