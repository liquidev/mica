# A trait can be implemented by a struct.

trait Animal
   func speak()
   func eat(food)
end

struct Dog impl
   func new(name) constructor = do
      @name = name
      @satiation = 0
   end

   as Animal
      func speak() = print("bark!")
      func eat(food) = do
         @satiation = @satiation + food
      end
   end
end
